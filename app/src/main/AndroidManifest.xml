<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.paulasmusicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowActivity"
            android:label="@string/category_listen_now" />
        <activity
            android:name=".HistoryActivity"
            android:label="@string/category_history" />
        <activity
            android:name=".LyricsActivity"
            android:label="@string/category_lyrics" />
        <activity
            android:name=".VideoActivity"
            android:label="@string/category_video" />
        <activity
            android:name=".BuyActivity"
            android:label="@string/category_buy" />

    </application>

</manifest>